{
  "name": "code-generator",
  "version": "1.0.0",
  "description": "代码生成器",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prestart": "node pre.js",
    "start-web": "ELEC_APP_TYPE=web electron .",
    "prestart-web": "node pre.js",

    "prepack-macos": "node pre.js && chmod +x ./go-build.sh && ./go-build.sh",
    "pack-macos": "electron-packager . CodeGin --icon=./icon.icns --platform=darwin --arch=x64 --out=build --overwrite"
  },
  "keywords": [
    "code-generator"
  ],
